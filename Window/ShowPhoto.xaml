<Window x:Class="Camera.ShowPhoto"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Camera"
        mc:Ignorable="d"
        WindowStartupLocation="CenterScreen"
        Title="ShowPhoto" Height="800" Width="800">
    <Grid>
        <Image x:Name="IMGBOX" Margin="0,0,0,50"></Image>
        <Button x:Name="PASSBTN" Click="PASSBTN_Click" Height="40" Width="150" Margin="50,0,0,0" HorizontalAlignment="Left" VerticalAlignment="Bottom" Content="PASS" FontSize="20" Foreground="White" Background="Green"></Button>
        <Button x:Name="FAILBTN" Click="FAILBTN_Click" Height="40" Width="150" Margin="250,0,0,0" HorizontalAlignment="Left" VerticalAlignment="Bottom" Content="FAIL" FontSize="20" Foreground="White" Background="Red"></Button>
        <Button x:Name="RETRYBTN" Click="RETRYBTN_Click" Height="40" Width="150" Margin="450,0,0,0" HorizontalAlignment="Left" VerticalAlignment="Bottom" Content="RETRY" FontSize="20" Foreground="White" Background="Gray"></Button>

    </Grid>
</Window>
